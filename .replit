modules = ["python-3.11"]
[agent]
integrations = ["web_scraper:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["file", "freetype", "glibcLocales", "xcodebuild", "zlib"]

[deployment]
deploymentTarget = "autoscale"
run = ["streamlit", "run", "app.py", "--server.port", "5000"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Modern Web Scraper"

[[workflows.workflow]]
name = "Modern Web Scraper"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "uvicorn main:app --host 0.0.0.0 --port 5000 --reload"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 40219
externalPort = 3001

[[ports]]
localPort = 41041
externalPort = 3002
